<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>pinterest clone feed</title>
    <link rel="stylesheet" href="/stylesheets/feed.css">
</head>
<body>
    
        <nav>
         <div class="logo">
           <img src="https://i.pinimg.com/originals/d3/d1/75/d3d175e560ae133f1ed5cd4ec173751a.png" alt="pin logo" class="img1" />
           <h5>Pinterest</h5>            
         </div>
         <ul class="navLink">
           <li><a href="/profile">Profile</a></li>
         </ul>
         <div class="lines">
             <div class="l1"></div>
             <div class="l2"></div>
             <div class="l3"></div>
         </div>
       </nav>
   
   <header style="word-spacing: 10px;">
     <h1>WELCOME TO FEED</h1>
   </header> 
   
     
   <div class="container">
         <% allPosts.forEach(function(elem){ %>
          <div class="box">
                          <a href="/images/uploads/<%= elem.image %>" >
                          <img src="/images/uploads/<%= elem.image %>" alt="image"></a>
                      <div class="caption"><%= elem.caption %> </div>
            </div>
         <% }) %>
   </div>
       
       
       <script src="https://code.jquery.com/jquery-3.3.1.js"></script>
         <script src="app.js"></script>

                     
   <script src="/javascripts/feed.js"></script>
</body>
</html>